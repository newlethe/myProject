<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.sgepit.pmis.finalAccounts.bdgStructure.hbm.FAOverallReport"
		table="fa_overall_report">
		<id name="uids" type="java.lang.String">
			<column name="uids" length="32" />
			<generator class="uuid"></generator>
		</id>
		<property name="pid" type="java.lang.String">
			<column name="PID" length="18"  />
		</property>
		<property name="faBdgid" type="java.lang.String">
			<column name="FA_BDGID" length="32"  />
		</property>
		<property name="faBdgname" type="java.lang.String">
			<column name="FA_BDGNAME" length="200"  />
		</property>
		<property name="buildBdgValue" type="java.math.BigDecimal">
			<column name="BUILD_BDG_VALUE" length="15" precision="4"  />
		</property>
		<property name="installBdgValue" type="java.math.BigDecimal">
			<column name="INSTALL_BDG_VALUE" length="15" precision="4"  />
		</property>
		<property name="equBdgValue" type="java.math.BigDecimal">
			<column name="EQU_BDG_VALUE" length="15" precision="4"  />
		</property>
		<property name="otherBdgValue" type="java.math.BigDecimal">
			<column name="OTHER_BDG_VALUE" length="15" precision="4"  />
		</property>
		<property name="totalBdgValue" type="java.math.BigDecimal">
			<column name="TOTAL_BDG_VALUE" length="15" precision="4"  />
		</property>
		<property name="buildRealValue" type="java.math.BigDecimal">
			<column name="BUILD_REAL_VALUE" length="15" precision="4"  />
		</property>
		<property name="installRealValue" type="java.math.BigDecimal">
			<column name="INSTALL_REAL_VALUE" length="15" precision="4"  />
		</property>
		<property name="equRealValue" type="java.math.BigDecimal">
			<column name="EQU_REAL_VALUE" length="15" precision="4"  />
		</property>
		<property name="otherRealValue" type="java.math.BigDecimal">
			<column name="OTHER_REAL_VALUE" length="15" precision="4"  />
		</property>
		<property name="totalRealValue" type="java.math.BigDecimal">
			<column name="TOTAL_REAL_VALUE" length="15" precision="4"  />
		</property>
		<property name="bdgRealDiff" type="java.math.BigDecimal">
			<column name="BDG_REAL_DIFF" length="15" precision="4"  />
		</property>
		<property name="bdgRealDiffPer" type="java.lang.Double">
			<column name="BDG_REAL_DIFF_PER" length="7" precision="4"  />
		</property>
		
	</class>

</hibernate-mapping>