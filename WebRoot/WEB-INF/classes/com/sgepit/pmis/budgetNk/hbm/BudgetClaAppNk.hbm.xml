<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	 <class name="com.sgepit.pmis.budgetNk.hbm.BudgetClaAppNk" table="BDG_CLA_APP_NK">
	    <id name="claappid" type="java.lang.String">
	        <column name="CLAAPPID" length="32" />
	        <generator class="uuid" />
	    </id>
	    <property name="conid" type="java.lang.String">
	        <column name="CONID" length="32" />
	    </property>
	    <property name="bdgid" type="java.lang.String">
	        <column name="BDGID" length="32" />
	    </property>
	    <property name="claid" type="java.lang.String">
	        <column name="CLAID" length="32" />
	    </property>
	    <property name="pid" type="java.lang.String">
	        <column name="PID" length="18" />
	    </property>
	    <property name="clamoney" type="java.lang.Double">
	        <column name="CLAMONEY" precision="15" scale="4" />
	    </property>
	    <property name="isLeaf" type="java.lang.Boolean">
	        <column name="IS_LEAF" precision="1" scale="0" />
	    </property>
	    <property name="parent" type="java.lang.String">
	        <column name="PARENT" length="32" />
	    </property>
	</class>
</hibernate-mapping>
