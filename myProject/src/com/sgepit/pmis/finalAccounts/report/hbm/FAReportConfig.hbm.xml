<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	
	<class name="com.sgepit.pmis.finalAccounts.report.hbm.FAReportConfig"
		table="fa_report_config">
		<id name="uids" type="java.lang.String">
			<column name="UIDS" length="32" />
			<generator class="uuid" />
		</id>
		<property name="reportModuleName" type="java.lang.String">
			<column name="REPORT_MODULE_NAME" length="100" />			
		</property>
		<property name="financialSource" type="java.lang.String">
			<column name="FINANCIAL_SOURCE" length="50" />			
		</property>
		<property name="bdgLevel" type="java.lang.String">
			<column name="BDG_LEVEL" length="32" />
		</property>
		
	</class>

</hibernate-mapping>