  <Chart type="AMCOLUMNCHART" width="680" height="200" text="柱状图" UID="2BO0G7J7" x="20" y="50">
    <param name="dataProvider" mode="sql"><![CDATA[SELECT a.y sj,nvl(b.tzplan,0) tzplan,nvl(b.zjin,0) zjin,nvl(b.zjout,0) zjout,nvl(b.tzcomp,0) tzcomp,nvl(b.yue,0) yue FROM (
SELECT to_char(ADD_MONTHS(SYSDATE,-12*2),'YYYY')||'年' Y FROM DUAL 
UNION
SELECT to_char(ADD_MONTHS(SYSDATE,-12*1),'YYYY')||'年' Y FROM DUAL 
UNION
SELECT to_char(SYSDATE,'YYYY')||'年' Y FROM DUAL 
UNION
SELECT to_char(ADD_MONTHS(SYSDATE,12),'YYYY')||'年' Y FROM DUAL 
) a
LEFT JOIN
(
SELECT 
v.prj_money_total tzplan,
t.year_in zjin,
t.year_out zjout,
t.year_comp tzcomp,
(T.TOTAL_IN - T.TOTAL_OUT) yue,
substr(t.sj_type,0,4)||'年' sj,
t.unit_id
 FROM pc_tzgk_month_comp_detail t ,v_pc_tzgk_month_report_zhxx v
 WHERE 1=1 
 AND v.unit_id = t.unit_id
 AND t.sj_type = v.sj_type 
 AND t.sj_type IN (SELECT max(m.sj_type) FROM pc_tzgl_month_comp_m m GROUP BY substr(m.sj_type,0,4)) 
 ) b
ON a.y = b.sj
ORDER BY a.y]]></param>
    <param name="depth3D" mode="style" value="20"/>
    <param name="angle" mode="style" value="45"/>
    <param name="categoryField" mode="prop" value="SJ"/>
    <param name="fields" mode="prop" value="TZPLAN|ZJIN|ZJOUT|TZCOMP|YUE"/>
    <param name="marginBottom" mode="style" value="25"/>
    <param name="marginTop" mode="style" value="0"/>
    <param name="marginRight" mode="style" value="0"/>
    <param name="marginLeft" mode="style" value="60"/>
