    <param name="rotate" mode="prop" value="false"/>
    <param name="columnSpacing" mode="style" value="0"/>
    <param name="graphs.title" mode="prop" value="投资计划|资金到位|资金支出|投资完成|资金余额"/>
    <param name="ygridCount" mode="prop" value="8"/>
  </Chart>
