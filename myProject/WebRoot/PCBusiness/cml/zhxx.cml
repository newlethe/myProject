  </variable>
  <Chart type="AMPIECHART" width="355" height="264" UID="IOSQL3" x="-1" y="25">
    <param name="dataProvider" mode="sql"><![CDATA[select round(SUM(t2.INVEST_SCALE)/10000,2) B,t2.PRJ_TYPE_NAME A  from v_Pc_Zhxx_Prj_Info t2 where t2.pid in (select t1.unitid  from sgcc_ini_unit t1 where t1.unit_type_id = 'A'   connect by prior t1.unitid = t1.upunit  start with t1.unitid = '[UNITID]') and  t2.PRJ_TYPE is not null group by t2.PRJ_TYPE_NAME]]></param>
    <param name="titleField" mode="prop" value="A"/>
    <param name="valueField" mode="prop" value="B"/>
    <param name="angle" mode="style" value="1"/>
    <param name="labelRadius" mode="style" value="-17"/>
    <param name="GraphDoubleClick" mode="event" value="scaleZoom"/>
    <param name="slicesColors" mode="prop" value="16763904|13421670|16764108|13408767"/>
    <param name="fontSize" mode="style" value="16"/>
    <param name="fields" mode="prop" value="B"/>
  </Chart>
  <Chart type="LABEL" width="381" height="28" UID="IOSS21" x="-4" y="0">
