  <Chart type="COLUMNCHART" width="888" height="256" UID="70LT3A7F" x="373" y="27">
    <param name="dataProvider" mode="sql"><![CDATA[select  'ztz' property_code,        '总投资'  property_name,        (select round(sum(v0.INVEST_SCALE)/10000,2) from v_pc_zhxx_prj_info v0 where v0.pid in (        select t1.unitid  from sgcc_ini_unit t1 where t1.unit_type_id = 'A'           connect by prior t1.unitid = t1.upunit  start with t1.unitid = '[UNITID]')         and v0.BUILD_NATURE = 'XJ') xj,         (select round(sum(v0.INVEST_SCALE)/10000,2) from v_pc_zhxx_prj_info v0 where v0.pid in (        select t1.unitid  from sgcc_ini_unit t1 where t1.unit_type_id = 'A'           connect by prior t1.unitid = t1.upunit  start with t1.unitid = '[UNITID]')         and v0.BUILD_NATURE = 'GJ') gj,         (select round(sum(v0.INVEST_SCALE)/10000,2) from v_pc_zhxx_prj_info v0 where v0.pid in (        select t1.unitid  from sgcc_ini_unit t1 where t1.unit_type_id = 'A'           connect by prior t1.unitid = t1.upunit  start with t1.unitid = '[UNITID]')         and v0.BUILD_NATURE = 'KJ') kj,         -1000 item_id    from dual      union allselect v1.property_code,        v1.property_name,       (select round(sum(src.AMOUNT)/10000,2) from PC_ZHXX_PRJ_FUNDSRC src where src.pid in(        select v0.PID from v_pc_zhxx_prj_info v0 where v0.pid in (        select t1.unitid  from sgcc_ini_unit t1 where t1.unit_type_id = 'A'           connect by prior t1.unitid = t1.upunit  start with t1.unitid = '[UNITID]')         and v0.BUILD_NATURE = 'XJ') and src.src_type=v1.PROPERTY_CODE       )  xj,       (select round(sum(src.AMOUNT)/10000,2) from PC_ZHXX_PRJ_FUNDSRC src where src.pid in(        select v0.PID from v_pc_zhxx_prj_info v0 where v0.pid in (        select t1.unitid  from sgcc_ini_unit t1 where t1.unit_type_id = 'A'           connect by prior t1.unitid = t1.upunit  start with t1.unitid = '[UNITID]')         and v0.BUILD_NATURE = 'GJ') and src.src_type=v1.PROPERTY_CODE       )  gj,       (select round(sum(src.AMOUNT)/10000,2) from PC_ZHXX_PRJ_FUNDSRC src where src.pid in(        select v0.PID from v_pc_zhxx_prj_info v0 where v0.pid in (        select t1.unitid  from sgcc_ini_unit t1 where t1.unit_type_id = 'A'           connect by prior t1.unitid = t1.upunit  start with t1.unitid = '[UNITID]')         and v0.BUILD_NATURE = 'KJ') and src.src_type=v1.PROPERTY_CODE       )  kj,       v1.ITEM_ID item_id    from  v_property v1 where v1.TNAME='资金类型' order by ITEM_ID asc]]></param>
    <param name="categoryField" mode="prop" value="PROPERTY_NAME"/>
    <param name="fields" mode="prop" value="XJ|GJ|KJ"/>
    <param name="GraphDoubleClick" mode="event" value="scaleZoom"/>
    <param name="series.displayName" mode="prop" value="新建|改建|扩建"/>
    <param name="series.fill:SolidColor.color" mode="style" value="16764006|13421670|16764108"/>
    <param name="showDataTips" mode="prop" value="true"/>
  </Chart>
