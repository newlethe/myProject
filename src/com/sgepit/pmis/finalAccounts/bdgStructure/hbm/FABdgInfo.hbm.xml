<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.sgepit.pmis.finalAccounts.bdgStructure.hbm.FABdgInfo"
		table="fa_bdg_info">
		<id name="bdgid" type="java.lang.String">
			<column name="bdgid" length="32" />
			<generator class="uuid"></generator>
		</id>
		<property name="pid" type="java.lang.String">
			<column name="PID" length="32"  />
		</property>
		<property name="bdgno" type="java.lang.String">
			<column name="BDGNO" length="32"  />
		</property>
		<property name="bdgname" type="java.lang.String">
			<column name="BDGNAME" length="600"  />
		</property>
		<property name="gcType" type="java.lang.String">
			<column name="GC_TYPE" length="32"  />
		</property>
		<property name="buildbdg" type="java.lang.String">
			<column name="BUILDBDG" length="32"  />
		</property>
		<property name="equipbdg" type="java.lang.String">
			<column name="EQUIPBDG" length="32"  />
		</property>
		<property name="installbdg" type="java.lang.String">
			<column name="INSTALLBDG" length="32"  />
		</property>
		<property name="otherbdg" type="java.lang.String">
			<column name="OTHERBDG" length="32"  />
		</property>
		<property name="isLeaf" type="java.lang.Boolean">
			<column name="IS_LEAF" length="32"  />
		</property>
		<property name="parent" type="java.lang.String">
			<column name="PARENT" length="32"  />
		</property>
		<property name="correspondbdg" type="java.lang.String">
			<column name="CORRESPONDBDG" length="32"  />
		</property>
		
	</class>

</hibernate-mapping>