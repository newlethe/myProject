<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.sgepit.pmis.finalAccounts.complete.hbm.FACompInfoOve"
		table="FACOMP_INFO_OVE">
		<id name="uids" type="java.lang.String">
			<column name="UIDS" length="32" />
			<generator class="uuid" />
		</id>
		<property name="pid" type="java.lang.String">
			<column name="PID" length="32" />
		</property>
		<property name="prjName" type="java.lang.String">
			<column name="PRJ_NAME" length="100"  />
		</property>
		<property name="prjAddress" type="java.lang.String">
			<column name="PRJ_ADDRESS" length="200"></column>
		</property>
		<property name="designUnit" type="java.lang.String">
			<column name="DESIGN_UNIT" length="100"></column>
		</property>
		<property name="constructUnit" type="java.lang.String">
			<column name="CONSTRUCT_UNIT" length="100"></column>
		</property>
		<property name="eqIntensity" type="java.lang.String">
			<column name="EQ_INTENSITY" length="20"></column>
		</property>
		<property name="fdIntensity" type="java.lang.String">
			<column name="FD_INTENSITY" length="20"></column>
		</property>
		<property name="qualityDeter" type="java.lang.String">
			<column name="QUALITY_DETER" length="300"></column>
		</property>
		<property name="buildProper" type="java.lang.String">
			<column name="BUILD_PROPER" length="100"></column>
		</property>
		<property name="supervision" type="java.lang.String">
			<column name="SUPERVISION" length="100"></column>
		</property>
		<property name="ratifyOrgan" type="java.lang.String">
			<column name="RATIFY_ORGAN" length="100"></column>
		</property>
	</class>
</hibernate-mapping>